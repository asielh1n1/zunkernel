[
  {
    "url": "/{{model}}",
    "method": "post",
    "controller":"{{model}}",
    "fn": "{{model|capitalize}}.create",
    "authenticated":"{{auth}}",
    "roles":""
  },
  {
    "url": "/{{model}}/:offset/:limit",
    "method": "get",
    "controller":"{{model}}",
    "fn": "{{model|capitalize}}.list",
    "authenticated":"{{auth}}",
    "roles":""
  },
  {
    "url": "/{{model}}/:id",
    "method": "get",
    "controller":"{{model}}",
    "fn": "{{model|capitalize}}.getById",
    "authenticated":"{{auth}}",
    "roles":""
  },
  {
    "url": "/{{model}}/:id",
    "method": "put",
    "controller":"{{model}}",
    "fn": "{{model|capitalize}}.update",
    "authenticated":"{{auth}}",
    "roles":""
  },
  {
    "url": "/{{model}}/:id",
    "method": "delete",
    "controller":"{{model}}",
    "fn": "{{model|capitalize}}.delete",
    "authenticated":false,
    "roles":""
  }
]